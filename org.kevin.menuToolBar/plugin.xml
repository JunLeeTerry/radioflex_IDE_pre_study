<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<plugin>

   <extension
         id="application"
         point="org.eclipse.core.runtime.applications">
      <application>
         <run
               class="org.kevin.menutoolbar.Application">
         </run>
      </application>
   </extension>
   <extension
         point="org.eclipse.ui.perspectives">
      <perspective
            name="RCP Perspective"
            class="org.kevin.menutoolbar.Perspective"
            id="org.kevin.menuToolBar.perspective">
      </perspective>
   </extension>
   <extension
         point="org.eclipse.ui.menus">
      <menuContribution
            allPopups="false"
            locationURI="menu:org.eclipse.ui.main.menu">
         <menu
               label="File">
            <command
                  commandId="org.kevin.menuToolBar.command1"
                  label="New File ..."
                  style="push">
            </command>
            <command
                  commandId="org.kevin.menuToolBar.command1"
                  label="Open File ..."
                  style="push">
            </command>
            <command
                  commandId="org.kevin.menuToolBar.command2"
                  label="Close File ..."
                  style="push">
            </command>
            <separator
                  name="org.kevin.menuToolBar.separator1"
                  visible="true">
            </separator>
            <command
                  commandId="org.eclipse.ui.newWizard"
                  label="New Project ..."
                  style="push">
            </command>
            <command
                  commandId="org.kevin.menuToolBar.command4"
                  label="Open Project ..."
                  style="push">
            </command>
            <command
                  commandId="org.kevin.menuToolBar.command5"
                  label="Close Project ..."
                  style="push">
            </command>
            <separator
                  name="org.kevin.menuToolBar.separator2"
                  visible="true">
            </separator>
            <command
                  commandId="org.kevin.menuToolBar.command6"
                  label="Save File ..."
                  style="push">
            </command>
            <command
                  commandId="org.eclipse.ui.file.saveAs"
                  label="Save As ..."
                  style="push">
            </command>
            <separator
                  name="org.kevin.menuToolBar.separator3"
                  visible="true">
            </separator>
            <menu
                  label="Project Setting ...">
               <command
                     commandId="org.kevin.menuToolBar.command1"
                     label="Build Setting"
                     style="push">
               </command>
               <command
                     commandId="org.kevin.menuToolBar.command2"
                     label="Debug Setting"
                     style="push">
               </command>
            </menu>
            <command
                  commandId="org.eclipse.ui.file.exit"
                  label="Exit"
                  style="push">
            </command>
         </menu>
         <menu
               label="Edit">
            <command
                  commandId="org.eclipse.ui.edit.redo"
                  label="Redo"
                  style="push">
            </command>
            <command
                  commandId="org.eclipse.ui.edit.undo"
                  label="Undo"
                  style="push">
            </command>
            <separator
                  name="org.kevin.menuToolBar.separator4"
                  visible="true">
            </separator>
            <command
                  commandId="org.eclipse.ui.edit.copy"
                  label="Copy"
                  style="push">
            </command>
            <command
                  commandId="org.eclipse.ui.edit.paste"
                  label="Paste"
                  style="push">
            </command>
            <command
                  commandId="org.eclipse.ui.edit.cut"
                  label="Cut"
                  style="push">
            </command>
            <separator
                  name="org.kevin.menuToolBar.separator1"
                  visible="true">
            </separator>
            <command
                  commandId="org.eclipse.ui.edit.findReplace"
                  label="Find and Replace"
                  style="push">
            </command>
         </menu>
         <menu
               label="Debug">
            <command
                  commandId="org.eclipse.ui.project.buildAll"
                  label="Build"
                  style="push">
            </command>
            <separator
                  name="org.kevin.menuToolBar.separator2"
                  visible="true">
            </separator>
            <command
                  commandId="org.kevin.menuToolBar.command5"
                  label="Start"
                  style="push">
            </command>
            <command
                  commandId="org.kevin.menuToolBar.command6"
                  label="Stop"
                  style="push">
            </command>
            <command
                  commandId="org.kevin.menuToolBar.command7"
                  label="Continue"
                  style="push">
            </command>
            <command
                  commandId="org.kevin.menuToolBar.command8"
                  label="Step"
                  style="push">
            </command>
            <separator
                  name="org.kevin.menuToolBar.separator3"
                  visible="true">
            </separator>
            <command
                  commandId="org.kevin.menuToolBar.command9"
                  label="Set Breakpoint"
                  style="push">
            </command>
            <command
                  commandId="org.kevin.menuToolBar.command10"
                  label="Remove Breakpoint"
                  style="push">
            </command>
            <separator
                  name="org.kevin.menuToolBar.separator4"
                  visible="true">
            </separator>
            <command
                  commandId="org.kevin.menuToolBar.command11"
                  label="Switch Mode"
                  style="push">
            </command>
         </menu>
         <menu
               label="Help">
            <command
                  commandId="org.eclipse.ui.help.aboutAction"
                  label="About"
                  style="push">
            </command>
         </menu>
      </menuContribution>
      <menuContribution
            allPopups="false"
            locationURI="toolbar:org.eclipse.ui.main.toolbar">
         <toolbar
               id="org.kevin.menuToolBar.toolbar1">
            <command
                  commandId="org.kevin.menuToolBar.command23"
                  label="1"
                  style="push">
            </command>
            <command
                  commandId="org.kevin.menuToolBar.command24"
                  label="2"
                  style="push">
            </command>
            <command
                  commandId="org.kevin.menuToolBar.command25"
                  label="3"
                  style="push">
            </command>
            <command
                  commandId="org.kevin.menuToolBar.command26"
                  label="4"
                  style="push">
            </command>
            <command
                  commandId="org.kevin.menuToolBar.command27"
                  label="5"
                  style="push">
            </command>
            <command
                  commandId="org.kevin.menuToolBar.command28"
                  label="6"
                  style="push">
            </command>
            <command
                  commandId="org.kevin.menuToolBar.command29"
                  label="7"
                  style="push">
            </command>
            <command
                  commandId="org.kevin.menuToolBar.command30"
                  label="8"
                  style="push">
            </command>
            <command
                  commandId="org.kevin.menuToolBar.command31"
                  label="9"
                  style="push">
            </command>
         </toolbar>
      </menuContribution>
   </extension>
   <extension
         point="org.eclipse.ui.handlers">
      <handler
            class="project.setting.BuildSettingHandle"
            commandId="org.kevin.menuToolBar.command1">
      </handler>
      <handler
            class="project.setting.DebugSettingHandle"
            commandId="org.kevin.menuToolBar.command2">
      </handler>
   </extension>

</plugin>
